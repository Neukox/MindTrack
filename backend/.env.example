# Exemplo de .env para desenvolvimento
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=mindtrack
POSTGRES_PASSWORD=your_password_here
POSTGRES_DB=mindtrack_db

# Substitua as placeholders abaixo. Exemplo:
# DATABASE_URL="postgresql://mindtrack:your_password_here@localhost:5432/mindtrack_db?schema=public"
DATABASE_URL="postgresql://mindtrack:your_password_here@localhost:5432/mindtrack_db?schema=public"

# Configurações da recuperação de senha
RESET_PASSWORD_TOKEN_EXPIRATION_MINUTES=15

# Configurações de servidor de e-mail
EMAIL_SERVER=smtp.your-email-provider.com
EMAIL_PORT=587
EMAIL_USER= # Seu e-mail aqui
EMAIL_PASS= # Sua senha ou app password aqui
EMAIL_FROM= # E-mail do remetente aqui
